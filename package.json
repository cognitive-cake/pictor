{
  "name": "pictor",
  "version": "1.0.0",
  "description": "pictor site refactoring",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.x",
    "autoprefixer": "^7.1.2",
    "browser-sync": "^2.18.2",
    "css-mqpacker": "^6.0.1",
    "del": "^3.0.0",
    "eslint": "^4.4.1",
    "eslint-config-htmlacademy": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-csso": "^3.0.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-svgmin": "^1.2.3",
    "gulp-svgstore": "^6.1.0",
    "run-sequence": "^2.1.0"
  },
  "scripts": {
    "test-html": "editorconfig-cli",
    "test-js": " eslint js/*",
    "build": "gulp build",
    "start": "gulp serve",
    "style": "gulp style-project"
  },
  "editorconfig-cli": [
    "*.html",
    "*.json",
    "*.js",
    "js/**/*.js",
    "img/**/*.svg",
    "sass/**/*.{sass,scss}"
  ],
  "eslintConfig": {
    "root": true,
    "extends": [
      "htmlacademy/es5"
    ],
    "plugins": [
      "html"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "define": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitive-cake/pictor.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cognitive-cake/pictor/issues"
  },
  "homepage": "https://github.com/cognitive-cake/pictor#readme"
}
